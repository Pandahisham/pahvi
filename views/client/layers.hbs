
<p class="help">{{ t "layers.help" }}</p>
<ul class="layersSortable" >
{{#each boxes}}
  <li class="{{this.type}}" data-id="{{this.id}}" >
  <div class="nameText" title="{{this.name}}">{{this.name}}</div>
  <div class="tools">
    <div class="visible fancyTooltip"  title='{{ t "layers.hide" }}' >
      <input type="checkbox"  {{#if this.visible}}checked{{/if}} />
    </div>
    <button class="delete fancyTooltip" title='{{ t "layers.delete" }}' >X</button>
  </div>
  </li>
{{/each}}
</ul>



